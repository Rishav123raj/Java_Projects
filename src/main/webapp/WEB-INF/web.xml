<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HomeController</servlet-name>
  	<display-name>HomeController</display-name>
  	<description></description>
  	<servlet-class>org.management.HomeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginData</servlet-name>
  	<display-name>LoginData</display-name>
  	<description></description>
  	<servlet-class>org.management.database.LoginData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DepositControl</servlet-name>
  	<display-name>DepositControl</display-name>
  	<description></description>
  	<servlet-class>org.management.deposit.DepositControl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WithdrawControl</servlet-name>
  	<display-name>WithdrawControl</display-name>
  	<description></description>
  	<servlet-class>org.management.wihtdraw.WithdrawControl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransferControl</servlet-name>
  	<display-name>TransferControl</display-name>
  	<description></description>
  	<servlet-class>org.management.transfer.TransferControl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CheckBalance</servlet-name>
  	<display-name>CheckBalance</display-name>
  	<description></description>
  	<servlet-class>org.management.deposit.BankAccountDAO.CheckBalance</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BalanceCheck</servlet-name>
  	<display-name>BalanceCheck</display-name>
  	<description></description>
  	<servlet-class>org.management.BalanceCheck</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HomeController</servlet-name>
  	<url-pattern>/HomeController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginData</servlet-name>
  	<url-pattern>/LoginData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DepositControl</servlet-name>
  	<url-pattern>/DepositControl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WithdrawControl</servlet-name>
  	<url-pattern>/WithdrawControl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransferControl</servlet-name>
  	<url-pattern>/TransferControl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CheckBalance</servlet-name>
  	<url-pattern>/CheckBalance</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BalanceCheck</servlet-name>
  	<url-pattern>/BalanceCheck</url-pattern>
  </servlet-mapping>
</web-app>
